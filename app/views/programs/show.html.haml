- model_class = Program
.page-header
  %h1=t '.title', :default => model_class.model_name.human

%p
  %strong= model_class.human_attribute_name(:user_id) + ':'
  %br
  = @program.user_id
%p
  %strong= model_class.human_attribute_name(:name) + ':'
  %br
  = @program.name
%p
  %strong= model_class.human_attribute_name(:normal_file_name) + ':'
  %br
  = @program.normal_file_name
%p
  %strong= model_class.human_attribute_name(:normal_file_contents) + ':'
  %br
  = @program.normal_file_contents
%p
  %strong= model_class.human_attribute_name(:super_file_contents) + ':'
  %br
  = @program.super_file_contents
%p
  %strong= model_class.human_attribute_name(:distill_file_contents) + ':'
  %br
  = @program.distill_file_contents
%p
  %strong= model_class.human_attribute_name(:arguments_file_name) + ':'
  %br
  = @program.arguments_file_name
%p
  %strong= model_class.human_attribute_name(:arguments_file_contents) + ':'
  %br
  = @program.arguments_file_contents
%p
  %strong= model_class.human_attribute_name(:number_of_levels) + ':'
  %br
  = @program.number_of_levels
%p
  %strong= model_class.human_attribute_name(:number_of_runs) + ':'
  %br
  = @program.number_of_runs

.form-actions
  = link_to t('.back', :default => t("helpers.links.back")), programs_path, :class => 'btn'
  = link_to t('.edit', :default => t("helpers.links.edit")), edit_program_path(@program), :class => 'btn'
  = link_to t('.destroy', :default => t("helpers.links.destroy")), program_path(@program), :method => "delete", :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')), :class => 'btn btn-danger'
